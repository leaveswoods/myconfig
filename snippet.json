{
	// Place your snippets for javascript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"pretty print javascript": {
		"prefix": "pp",
		"body": [
			"console.log(JSON.stringify(${1:documentName}, undefined, 2));"
		],
		"description": "pretty pring javascript"
	},

	"console.log javascript": {
		"prefix": "cl",
		"body": [
			"console.log(${1:document});"
		],
		"description": "console.log"
	},

	"arrow function javascript": {
		"prefix": "af",
		"body": [
			"const ${1:function name} = () => {",
			"",
			"};"
		],
		"description": "arrow function javascript"
	},

	"callback javascript": {
		"prefix": "cb",
		"body": [
			"(${1:document}) => {",
			"",
			"}"
		],
		"description": "callback javascript"
	},

	"require javascript": {
		"prefix": "rq",
		"body": [
			"const ${1:name} = require('${2:module}');"
		],
		"description": "require javascript"
	},

	"schema filed": {
		"prefix": "sf",
		"body": [
			"${1:filedName}: { type: ${2:fieldType}, required: true }, "
		],
		"description": "schema filed"
	},

	"controller function": {
		"prefix": "cf",
		"body": [
			"function (res, req) {",
			"${1:}",
			"};"
		],
		"description": "controller function"
	}
}